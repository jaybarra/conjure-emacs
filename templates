php-mode

(php "<?php" n> "?>")

emacs-lisp-mode

(module ";;; conjure-" (s module) ".el --- Configurations for " (capitalize (or module "")) " in Conjure -*- lexical-binding: t -*-" n
        ";;; Commentary:" n
        ";;; Code:" n q n
        "(provide 'conjure-" module ")" n
        ";;; conjure-" module ".el ends here")

clojure-mode

(ns "(ns " (clojure-expected-ns) ")")
